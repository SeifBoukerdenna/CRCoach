{
    "build": {
      "env": {
        "VITE_FORCE_REMOTE_URL": "true",
        "VITE_REMOTE_HOST": "35.208.133.112",
        "VITE_REMOTE_PORT": "8080",
        "VITE_USE_HTTPS": "false"
      }
    },
    "functions": {
      "app/api/**/*.js": {
        "runtime": "@vercel/node"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ]
  }