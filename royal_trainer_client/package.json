{
  "name": "royal-trainer-webapp",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:remote": "VITE_FORCE_REMOTE_URL=true vite --mode development",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "build:check": "tsc --noEmit && vite build --mode production",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:remote": "VITE_FORCE_REMOTE_URL=true vite preview",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:preview": "vercel"
  },
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.5.0",
    "typescript": "^5.2.2",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "terser": "^5.42.0"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "vercel": "^33.0.0"
  }
}